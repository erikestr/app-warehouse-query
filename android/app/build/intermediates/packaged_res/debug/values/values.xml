<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="air_force_blue">#013789</color>
    <color name="deep_carmine_pink">#FF3333</color>
    <color name="eucalyptus_green">#32EA8E</color>
    <color name="fire_opal_pink">#EE5B5B</color>
    <color name="gray">#A1A1A1</color>
    <color name="ic_launcher_background">#FFFFFF</color>
