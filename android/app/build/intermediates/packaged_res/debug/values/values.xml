    <color name="space_cadet_blue">#1B1464</color>
    <dimen name="border_corner">32dp</dimen>
    <string name="app_name">Jayor Consulta de Inventarios</string>
    <string name="custom_url_scheme">com.jayor.mx.warehouse.query</string>
